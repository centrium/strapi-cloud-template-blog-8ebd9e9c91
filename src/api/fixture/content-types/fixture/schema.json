{
  "kind": "collectionType",
  "collectionName": "fixtures",
  "info": {
    "singularName": "fixture",
    "pluralName": "fixtures",
    "displayName": "Fixture"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "matchweek": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::matchweek.matchweek",
      "inversedBy": "fixtures"
    },
    "home_team": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::team.team",
      "inversedBy": "fixtures_home"
    },
    "away_team": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::team.team",
      "inversedBy": "fixtures_away"
    },
    "date": {
      "type": "date"
    },
    "stadium": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::stadium.stadium",
      "inversedBy": "fixtures"
    },
    "referee": {
      "type": "string"
    },
    "home_score": {
      "type": "integer"
    },
    "away_score": {
      "type": "integer"
    },
    "attendance": {
      "type": "integer"
    },
    "fixture_status": {
      "type": "enumeration",
      "enum": [
        "Scheduled",
        "Postponed",
        "Completed",
        "Abandoned",
        "Awarded",
        "Cancelled"
      ]
    },
    "home_manager": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::manager.manager",
      "inversedBy": "fixture_home_manager"
    },
    "away_manager": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::manager.manager",
      "inversedBy": "fixture_manager_away"
    },
    "events": {
      "type": "component",
      "component": "fixtures.match-event",
      "repeatable": true
    }
  }
}
